{
  "name": "chat",
  "version": "1.0.0",
  "description": "My chat",
  "scripts": {
    "start": "npm run delete & npm run build & npm run start-server",
    "dev": "npm run delete & npm run watch & npm run start-server",
    "build": "parcel build ./src/index.html",
    "watch": "parcel watch ./src/index.html",
    "start-server": "node ./server.js",
    "delete": "rm -rf ./dist/  & rm -rf ./.parcel-cache/"
  },
  "parcelIgnore": [
    "/upload/.+"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MefiseRay/chat.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MefiseRay/chat/issues"
  },
  "homepage": "https://github.com/MefiseRay/chat#readme",
  "devDependencies": {
    "express": "4.18.1",
    "parcel": "2.7.0",
    "parcel-resolver-ignore": "2.1.3",
    "parcel-transformer-pug-precompile": "1.0.6",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "nanoid": "4.0.0",
    "pug": "3.0.2"
  }
}
