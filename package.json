{
  "name": "chat",
  "version": "1.0.0",
  "description": "My chat",
  "scripts": {
    "start": "npm run delete & npm run build & npm run start-server",
    "dev": "npm run delete & npm run watch & npm run start-server",
    "build": "parcel build ./src/index.html",
    "watch": "parcel watch ./src/index.html",
    "start-server": "node ./server.js",
    "delete": "rm -rf ./dist/  & rm -rf ./.parcel-cache/",
    "eslint": "./node_modules/.bin/eslint . --fix",
    "stylelint": "npx stylelint '**/*.scss'"
  },
  "parcelIgnore": [
    "/upload/.+"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MefiseRay/chat.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MefiseRay/chat/issues"
  },
  "homepage": "https://github.com/MefiseRay/chat#readme",
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "express": "4.18.1",
    "parcel": "2.7.0",
    "parcel-packager-svg-sprite": "2.0.0",
    "parcel-resolver-ignore": "2.1.3",
    "parcel-transformer-pug-precompile": "1.0.6",
    "parcel-transformer-svg-sprite": "2.0.0",
    "stylelint": "14.11.0",
    "stylelint-config-standard-scss": "5.0.0",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "dotenv": "^16.0.2",
    "nanoid": "4.0.0",
    "pug": "3.0.2"
  }
}
